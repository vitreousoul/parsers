{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g3",
                "${workspaceFolder}/parse_ical.c",
                "-o",
                "${workspaceFolder}/a.out",
                "-std=c89",
                "-Wall",
                "-Wextra",
                "-Wstrict-prototypes",
                "-Wold-style-definition",
                "-Wmissing-prototypes",
                "-Wmissing-declarations"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
    ],
    "version": "2.0.0"
}
